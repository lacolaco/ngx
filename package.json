{
  "name": "ngx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --progress false",
    "lint": "ng lint",
    "ci:test": "run-s lint build:libs test:ci",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "build:libs:ngx-grid-layout": "ng build ngx-grid-layout",
    "build:libs": "run-p build:libs:*",
    "publish:libs:ngx-grid-layout": "yarn publish --cwd dist/@lacolaco/ngx-grid-layout --access public",
    "publish:libs": "run-s publish:libs:*",
    "changelog:libs:ngx-grid-layout": "standard-changelog -i libs/ngx-grid-layout/CHANGELOG.md -k libs/ngx-grid-layout/package.json -o libs/ngx-grid-layout/CHANGELOG.md --commit-path libs/ngx-grid-layout",
    "changelog:libs": "run-p changelog:libs:*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.1.0",
    "@angular/common": "5.1.0",
    "@angular/compiler": "5.1.0",
    "@angular/core": "5.1.0",
    "@angular/forms": "5.1.0",
    "@angular/platform-browser": "5.1.0",
    "@angular/platform-browser-dynamic": "5.1.0",
    "@angular/router": "5.1.0",
    "@lacolaco/reactive-store": "^2.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.11",
    "@angular-devkit/build-ng-packagr": "^0.5.11",
    "@angular/cli": "^6.0.0-rc.8",
    "@angular/compiler-cli": "5.1.0",
    "@angular/language-service": "5.1.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.0",
    "standard-changelog": "^1.0.8",
    "ts-node": "~3.2.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.7.0",
    "typescript": "2.5.3"
  }
}
